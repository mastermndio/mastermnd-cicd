      "security/detect-possible-timing-attacks": "error",
      "security/detect-pseudoRandomBytes": "error",
      "space-before-function-paren": "off",
      "object-curly-spacing": "off"
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "ecmaFeatures": {
        "impliedStrict": true
      }
    }
